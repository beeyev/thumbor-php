{
    "name": "beeyev/thumbor-php",
    "description": "Thumbor library for PHP",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "Thumbor"
    ],
    "authors": [
        {
            "name": "Alexander Tebiev",
            "email": "tebiev@mail.com",
            "homepage": "https://github.com/beeyev/"
        }
    ],
    "homepage": "https://github.com/beeyev/thumbor-php",
    "support": {
        "issues": "https://github.com/beeyev/thumbor-php/issues",
        "source": "https://github.com/beeyev/thumbor-php/php.git",
        "docs": "https://github.com/beeyev/thumbor-php/",
        "rss": "https://github.com/beeyev/thumbor-php/releases.atom"
    },
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "require-dev": {
        "php": "^7.2 || ^8.0",
        "friendsofphp/php-cs-fixer": "~2.19",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.5 || ^9.5",
        "rector/rector": "^0.16.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Beeyev\\Thumbor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beeyev\\Thumbor\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Thumbor": "Beeyev\\Thumbor\\Adapters\\Laravel\\Facades\\ThumborFacade"
            },
            "providers": [
                "Beeyev\\Thumbor\\Adapters\\Laravel\\ThumborServiceProvider"
            ]
        }
    }
}
